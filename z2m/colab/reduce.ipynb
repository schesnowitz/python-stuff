        "nums = [1,10,2,20]\n",
        "\n",
        "def some_func(argu, item):\n",
        "    return argu + item\n",
        "\n",
        "\n",
        "# takes in function, data, initial value\n",
        "r = reduce(some_func, nums, 0)\n",
        "print(r)"
